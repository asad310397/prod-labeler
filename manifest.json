{
  "manifest_version": 3,
  "name": "Prod Labeler",
  "version": "1.0",
  "description": "Demonstrates interaction with the active tab.",
  "permissions": ["activeTab", "scripting", "tabs", "storage"],
  "action": {
    "default_icon": "assets/safe.png",
    "default_popup": "url_list.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
